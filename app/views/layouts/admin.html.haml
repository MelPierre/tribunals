!!!
%html
  %head
    = render "shared/head"
    = stylesheet_link_tag "admin", :media => "all"

  %body
    
    = render "shared/header"

    #wrapper.group.admin
      #global-breadcrumb.header-context
        %nav{:role => "navigation"}
          %ol.group
            %li= link_to 'Admin home', admin_decisions_path
            %li= link_to 'Log out', logout_admin_authentication_path
      %section#content.group{:role => "main"}
        .full-width-group
          = yield

    = render "shared/footer"

    = render "shared/script"
